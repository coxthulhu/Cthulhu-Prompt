{
  "name": "cthulhuprompt",
  "version": "0.0.8",
  "description": "An Electron application with Svelte and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.web.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "dev": "cross-env DEV_ENVIRONMENT=DEV CHOKIDAR_USEPOLLING=1 CHOKIDAR_INTERVAL=200 electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "test:vitest": "vitest run tests/vitest",
    "test:playwright": "cross-env VITE_LOG_LEVEL=warn npm run build && cross-env DEV_ENVIRONMENT=PLAYWRIGHT npx playwright test"
  },
  "dependencies": {
    "@codingame/monaco-vscode-api": "^26.2.1",
    "@codingame/monaco-vscode-configuration-service-override": "^26.2.1",
    "@codingame/monaco-vscode-languages-service-override": "^26.2.1",
    "@codingame/monaco-vscode-markdown-basics-default-extension": "^26.2.1",
    "@codingame/monaco-vscode-markdown-language-features-default-extension": "^26.2.1",
    "@codingame/monaco-vscode-model-service-override": "^26.2.1",
    "@codingame/monaco-vscode-textmate-service-override": "^26.2.1",
    "@codingame/monaco-vscode-theme-defaults-default-extension": "^26.2.1",
    "@codingame/monaco-vscode-theme-service-override": "^26.2.1",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/svelte-db": "^0.1.69",
    "electron-updater": "^6.3.9",
    "lucide-svelte": "^0.555.0",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@^26.2.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.11",
    "vscode": "npm:@codingame/monaco-vscode-extension-api@^26.2.1"
  },
  "devDependencies": {
    "@codingame/esbuild-import-meta-url-plugin": "^1.0.3",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.561.0",
    "@playwright/test": "^1.41.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/node": "^22.16.5",
    "@types/vscode": "^1.109.0",
    "bits-ui": "^2.14.4",
    "cross-env": "^10.1.0",
    "electron": "^37.2.3",
    "electron-builder": "^26.5.0",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-svelte": "^3.13.0",
    "memfs": "^4.6.0",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.53.0",
    "svelte-check": "^4.4.3",
    "tailwind-variants": "^3.2.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vitest": "^3.0.0"
  }
}
